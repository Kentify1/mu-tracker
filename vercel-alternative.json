{
  "version": 2,
  "builds": [
    {
      "src": "*.php",
      "use": "vercel-php@0.6.0"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.php)$",
      "dest": "/$1"
    },
    {
      "src": "/",
      "dest": "/index.php"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "APP_ENV": "production"
  }
}
